{
  "manifest_version": 2,
  "name": "Parent/Child Managment for Trello",
  "description": "Add support of Parent/Child dependencies in Trello projects.",
  "developer": {
    "name": "Handsome",
    "url": "http://handsome.is"
  },
  "homepage_url": "http://labs.handsome.is/trello/",
  "version": "0.0.0",
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://trello.com/"
  ],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/*"
      ],
      "css": [
        "css/style.css"
      ],
      "js": [
        "js/vendor/jquery.min.js",
        "js/vendor/jquery-ui.min.js",
        "js/config.js",
        "js/settings.js",
        "js/api.js",
        "js/plugins/inheritance.js",
        "js/init.js"
      ]
    }
  ]
}
